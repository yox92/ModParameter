{
  "compilerOptions": {
    "module": "commonjs",
    "outDir": "./dist",
    "rootDir": "./",
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "allowJs": true,
    "baseUrl": "src",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "target": "es6",
    "paths": {
      "@spt/*": ["../types/spt/*"],
      "@/*": ["./*"],
       "@spt-aki/*": ["external/server/project/src/*"]
    }
  },
  "include": [
    "src"
  ]
}